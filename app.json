{
  "expo": {
    "name": "WellSync Smart Health",
    "slug": "wellsync-health",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wellsync.health"
    },
    "android": {
      "package": "com.wellsync.health",
      "versionCode": 1,
      "permissions": [
        "ACTIVITY_RECOGNITION",
        "ACCESS_FINE_LOCATION",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "wellsync",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-web-browser"
    ]
  }
}
